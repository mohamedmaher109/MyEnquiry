@model ICollection<MyEnquiry_BussniessLayer.ViewModel.UserView>
<table class="table datatables" id="dataTable-2">
    <thead>
        <tr>
            <th>الكود </th>
            <th>الاسم </th>
            <th>البريد الالكترونى</th>
            <th>رقم الهاتف</th>
            <th>اسم المدينة </th>
            <th>اسم المؤهل </th>
            <th>العنوان </th>
            <th>الرقم القومى </th>
            @*<th>مناطق التغطية </th>*@
            <th>اسم الشركة </th>

           
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                  <td>@item.Id.Substring(0,4)</td>
                <td> <label class="text-danger-dark">@item.FullName</label>  </td>
                <td>@item.Email</td>
                <td>@item.Phone</td>
                <td>@item.City</td>
                <td>@item.Education</td>
                <td>@item.Address</td>
                <td>@item.NationalId</td>
                @*<td>@item.Coveragenames</td>*@
                <td>@item.CompanyName</td>
                


             
            </tr>
        }



    </tbody>
</table>

