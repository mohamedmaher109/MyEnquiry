<script>
    
function COmpanyById() {
	var id2 = $('#Adsd').val();
	$.ajax({
		url: "/BankCompanyCase/DisplayGridBancCompany",
		cache: false,
		data: { 'id': id2 },
		success: function (html) {
			$(".dispalygridpas").html(html);

			$('#dataTable-pas').DataTable(
				{
					autoWidth: true,
					"lengthMenu": [
						[10, 20, 40, -1],
						[10, 20, 40, "All"]
					]
				});

		}
	});
}
$(document).ready(function () {

	COmpanyById();


})
</script>

