@{
}
@model ICollection<MyEnquiry_DataLayer.Models.CasesTypeForms>
<table class="table datatables" id="dataTable-1">
    <thead>
        <tr>
            <th>نوع الحالة</th>
            
            <th>حذف</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.GroupBy(s=>s.CaseTypeId).ToList())
        {
        <tr>


            <td>@item.FirstOrDefault().CaseType.NameAr</td>
            
            <td>
                <button type="button" data-id="@item.FirstOrDefault().CaseTypeId" class="btn mb-2 btn-outline-danger delete">حذف</button>

            </td>
        </tr>
        }



    </tbody>
</table>